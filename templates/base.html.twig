<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}

</head>
<body>
{% block body %}
    <main id="root" class="container" data-options="{{ app_settings | json_encode | e('html_attr') }}">
        {% block content %}{% endblock %}
    </main>
{% endblock %}
</body>
</html>
